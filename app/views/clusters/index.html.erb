<div>
  <h1>Cluster</h1>
</div>

<%= will_paginate %>

<table class="table table-striped table-bordered table-condensed">
  <thead>
  <tr>
    <th colspan="1" width="20">ID</th>
    <th colspan="5">Cluster</th>
  </tr>
  </thead>
  <tbody>
  <% @clusters.each do |cluster| %>
    <tr>
      <td><%= cluster.id %></td>
      <td><%= link_to cluster.name, cluster %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= will_paginate %>
